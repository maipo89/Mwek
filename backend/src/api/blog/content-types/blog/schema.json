      "target": "api::categorie.categorie",
      "inversedBy": "blogs"
    },
    "content": {
      "type": "richtext"
    },
    "description": {
      "type": "text"
    },
    "dynamic_content": {
      "type": "dynamiczone",
      "components": [
        "text-content-blocks.content-block",
        "text-content-blocks.content-with-image"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categorie.categorie"
    },
    "PrimaryCategory": {
      "type": "component",
      "repeatable": false,
      "component": "blogs.primary-category",
      "required": true
